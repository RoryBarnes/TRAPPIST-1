# Planet h parameters
sName                     h              # Body's name
saModules                 spinbody       # Modules to apply, exact spelling required

saOutputOrder Time SemiMajorAxis Eccentricity SpiNBodyInc LongP SpiNBodyLongA MeanAnomaly -Instellation

# Physical Properties (from Grimm et al. [2018])
dMass                     -0.331           # Mass, negative -> Earth masses
dRadius                   -0.719           # Radius, negative -> Earth radii

# SpiNBody Properties
bUseOrbParams                    1

# Orbital Properties
dSemi                     -0.06193488     # Semi-major axis, negative -> AU
dEcc                       0.00567        # Eccentricity
dInc                       0.02           # Inclination (deg)
dLongA                     4.5          # Longitude of ascending node (deg)
dArgP                      338.92         # Argument of pericenter (deg)
dMeanA                     269.72         # Mean Anomaly (deg)
